            android:textSize="24sp"
            app:backgroundTint="@android:color/holo_orange_dark"
            app:layout_constraintStart_toEndOf="@+id/button19"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button19"
            android:layout_width="60dp"
            android:layout_height="50dp"
            android:background="@drawable/button_layout"
            android:onClick="bracket_closeClick"
            android:text="@string/Bracket_close"
            android:textSize="24sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toEndOf="@+id/button6"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button20"
            android:layout_width="60dp"
            android:layout_height="50dp"
            android:background="@drawable/button_layout"
            android:onClick="clearClick"
            android:text="@string/Clear"
            android:textSize="24sp"
            app:backgroundTint="#FF0000"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="end"
        android:hint="@string/Expression"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:textColorHint="@android:color/darker_gray"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/constraintLayout"
        app:layout_constraintStart_toStartOf="parent">

        <Button
            android:id="@+id/button21"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:background="@drawable/button_layout"
            android:onClick="sinFunction"
            android:text="@string/sinFunction"
            android:textAllCaps="false"
            android:textSize="15sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button22"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:background="@drawable/button_layout"
            android:onClick="combinationFunction"
            android:text="@string/nCr"
            android:textAllCaps="false"
            android:textSize="15sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toEndOf="@+id/button23"
            app:layout_constraintTop_toBottomOf="@+id/button26" />

        <Button
            android:id="@+id/button23"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:background="@drawable/button_layout"
            android:onClick="permutationFunction"
            android:text="@string/nPr"
            android:textAllCaps="false"
            android:textSize="15sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toEndOf="@+id/button24"
            app:layout_constraintTop_toBottomOf="@+id/button30" />

        <Button
            android:id="@+id/button24"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:background="@drawable/button_layout"
            android:onClick="logFunction"
            android:text="@string/logFunction"
            android:textAllCaps="false"
            android:textSize="13sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button29" />

        <Button
            android:id="@+id/button25"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:background="@drawable/button_layout"
            android:onClick="lnFunction"
            android:text="@string/naturalLog"
            android:textAllCaps="false"
            android:textSize="12sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toEndOf="@+id/button26"
            app:layout_constraintTop_toBottomOf="@+id/button31" />

        <Button
            android:id="@+id/button26"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:background="@drawable/button_layout"
            android:onClick="tanInverseFunction"
            android:text="@string/tanInverse"
            android:textAllCaps="false"
            android:textSize="13sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toEndOf="@+id/button30"
            app:layout_constraintTop_toBottomOf="@+id/button28" />

        <Button
            android:id="@+id/button27"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:background="@drawable/button_layout"
            android:onClick="cosFunction"
            android:text="@string/cosFunction"
            android:textAllCaps="false"
            android:textSize="15sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toEndOf="@+id/button21"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button28"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:background="@drawable/button_layout"
            android:onClick="tanFunction"
            android:text="@string/tanFunction"
            android:textAllCaps="false"
            android:textSize="15sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toEndOf="@+id/button27"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button29"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:background="@drawable/button_layout"
            android:onClick="sinInverseFunction"
            android:text="@string/sinInverse"
            android:textAllCaps="false"
            android:textSize="13sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button21" />

        <Button
            android:id="@+id/button30"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:background="@drawable/button_layout"
            android:onClick="cosInverseFunction"
            android:text="@string/cosInverse"
            android:textAllCaps="false"
            android:textSize="13sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toEndOf="@+id/button29"
            app:layout_constraintTop_toBottomOf="@+id/button27" />

        <Button
            android:id="@+id/button31"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:background="@drawable/button_layout"
            android:onClick="XpowerYFunction"
            android:text="@string/XpowerY"
            android:textAllCaps="false"
            android:textSize="18sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toEndOf="@+id/button28"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button32"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:background="@drawable/button_layout"
            android:onClick="ApowerBdivideCFunction"
            android:text="@string/ApowerBdivideC"
            android:textAllCaps="false"
            android:textSize="16sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button24" />

        <Button
            android:id="@+id/button33"
            android:layout_width="60dp"
            android:layout_height="50dp"
            android:background="@drawable/button_layout"
            android:text="null"
            android:textSize="12sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toEndOf="@+id/button32"
            app:layout_constraintTop_toBottomOf="@+id/button23" />

        <Button
            android:id="@+id/button34"
            android:layout_width="60dp"
            android:layout_height="50dp"
            android:background="@drawable/button_layout"
            android:text="null"
            android:textSize="12sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toEndOf="@+id/button33"
            app:layout_constraintTop_toBottomOf="@+id/button22" />

        <Button
            android:id="@+id/button35"
            android:layout_width="60dp"
            android:layout_height="50dp"
            android:background="@drawable/button_layout"
            android:text="null"
            android:textSize="12sp"
            app:backgroundTint="@android:color/darker_gray"
            app:layout_constraintStart_toEndOf="@+id/button34"
